    <outline showAllTags="true">
        <taglist>
            <![CDATA[cfscript,cfcomponent,cffunction,cfinterface ]]>
        </taglist>
    </outline>

    <typing>
    	<insert autoinsertmatchingchar="true" autoinsertdqoutes="true" autoinsertsqoutes="true" autoinsertroundbracket="true" autoinsertsquarebracket="true" autoinsertcurlybracket="true" autoinserthash="true" />
    	<close autoclosetags="autoendwithslash" />
    	<indent autoindentcarriagereturn="true" autoindenttyping="true" replacewhitespace="true" />
   	</typing>
    <!-- <typing autoinsertmatchingchar="true" modifyPair="automodify" autoindent="autoindent" /> -->

