        <command name="com.adobe.ide.coldfusion.keyboard.installExtension" contextID="org.eclipse.ui.contexts.window" key="M1+SHIFT+E I"/>
        <command name="com.adobe.ide.coldfusion.keyboard.anchorCmd" contextID="com.adobe.editor.cfml.context" key="M1+T L"/>
