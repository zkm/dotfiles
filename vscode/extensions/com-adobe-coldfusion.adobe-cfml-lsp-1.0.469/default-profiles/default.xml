        <command name="com.adobe.ide.coldfusion.shorucut.cfoutput" contextID="com.adobe.editor.cfml.context" key="M1+T O"/>
		<command name="com.adobe.ide.coldfusion.shortcut.cfcomment" contextID="com.adobe.editor.cfml.context" key="M1+T M"/>
		<command name="com.adobe.ide.coldfusion.shortcut.cfscriptcomment" contextID="com.adobe.editor.cfml.context" key="M1+Shift+8"/>
