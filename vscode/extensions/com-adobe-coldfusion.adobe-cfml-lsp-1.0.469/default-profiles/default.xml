<?xml version="1.0" encoding="UTF-8"?>
<profile name="Default" systemprofile = "true">
    <codeassist dictionaryVersion="ColdFusion2023">
        <autoactivation enabled="true" delay="50" />
        <autoinsert singleProposal="false" autoCloseTag="true" insertClosingTagOnNewLine="true" autoQuote="true" appendspace="true" insertequalsign="true" enablesqlcodeassist="true" inserttagattributes="true">

        <quotecharacter>
            <![CDATA["]]>
        </quotecharacter>
        </autoinsert>
        <filter cycleproposal="true" filterproposalwithtext = "false" />
    </codeassist>

    <generaleditor showToolBar="true" highlightcolor="192,192,192" pairmatching="both" enablewordwrap = "true" trimspacebeforesave="false" >
        <newfilesettings type="CFM" defaultName="Untitled_cfm">
            <defaultContent><![CDATA[]]></defaultContent>
        </newfilesettings>
        <newfilesettings type="CFC" defaultName="Untitled_cfc">
            <defaultContent><![CDATA[]]></defaultContent>
        </newfilesettings>
    </generaleditor>

    <keys>
