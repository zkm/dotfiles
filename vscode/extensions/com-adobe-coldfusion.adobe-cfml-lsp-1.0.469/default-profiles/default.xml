    <syntaxchecking enablesyntaxchecking="true" enablequickfix="false" erroronsaveonly="false" />

	<tasktags>
		<tag name="TODO" priority="Normal"></tag>
		<tag name="FIXME" priority="High"></tag>
    </tasktags>

